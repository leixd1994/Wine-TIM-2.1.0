Au(auinfodata)}}if(document.getElementById("CoAuChartdata")){var chartref=document.getElementById("CoAuChartdata").value;if(chartref!=""){RenderCoAuChart(chartref)}}break;case"602":if(document.getElementById("kwinfodata")){var kwinfodata=document.getElementById("kwinfodata").value;if(kwinfodata!=""){kwinfodata=eval(kwinfodata);RenderLineChart(kwinfodata)}}break;case"607":if(document.getElementById("refchartdata")){var chartref=document.getElementById("refchartdata").value;if(chartref!=""){RenderRefChart(chartref)}}break;case"614":if(document.getElementById("CoAuChartdata")){var chartref=document.getElementById("CoAuChartdata").value;if(chartref!=""){RenderCoAuChart(chartref)}}break;default:break}}}function showFlash(a){if(a){$(".active").removeClass("active");$(a).addClass("active")}$("#MapArea").hide();$("#frame1").hide();if($("#func607").text()!=""){$("#func607").show()}else{LoadFilesFromId("catalog_func607");$("#func607").show()}if($("#framecatalog_CkFiles").attr("src")==""&&$("#framecatalog_CkFiles").attr("height")==0){LoadFilesFromId("catalog_CkFiles");$("#framecatalog_CkFiles").show()}else{$("#framecatalog_CkFiles").show()}if($("#framecatalog_YzFiles").attr("src")==""&&$("#framecatalog_YzFiles").attr("height")==0){LoadFilesFromId("catalog_YzFiles");$("#framecatalog_YzFiles").show()}else{$("#framecatalog_YzFiles").show()}TurnToTitle("catalog_ref")}function showRefChart(a){if(a){$(".active").removeClass("active");$(a).addClass("active")}$("#MapArea").show();$("#frame1").show();$("#func607").hide();$("#framecatalog_CkFiles").hide();$("#framecatalog_YzFiles").hide()};